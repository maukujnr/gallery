/* Requires the Docker Pipeline plugin */
pipeline {
    agent any 
    stages {
        stage('clone') {
            steps {
                sh 'node --version'
            }
        }
        stage('build') {
            steps {
                sh 'node --version'
            }
        }
        stage('test') {
            steps {
                sh 'node --version'
            }
        }
        stage('deploy') {
            steps {
                sh 'node --version'
            }
        }
    }
}